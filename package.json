{
  "name": "wish-a-link",
  "version": "0.0.1",
  "author": "Wish a Link",
  "homepage": "https://wishalink.com/",
  "description": "Wish a Link - Mobile",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ionic build --prod",
    "sync": "ionic capacitor sync && node ./api/version.js",
    "resources": "cordova-res ios && cordova-res android && node ./api/resources.js",
    "ios-run-l": "ionic capacitor run ios -l --external",
    "android-run-l": "ionic capacitor run android -l --external",
    "android-open": "ionic capacitor update android && npx cap open android",
    "ios-open": "ionic capacitor update ios && npx cap open ios",
    "jetify": "npx jetify && npx cap sync android",
    "nswag-gen-kpay-backend": "nswag openapi2tsclient /input:https://api.kpay.com.tr/appbackend/KPay.API.AppBackend.V1.yaml /output:src/app/services/api-kpay-backend.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromOperationId /ClassName:{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:KPAY_BACKEND_API_URL && node ./api/api.js",
    "nswag-gen-kpay-ccpayment": "nswag openapi2tsclient /input:https://api.kpay.com.tr/ccpayment/KPay.API.CCPayment.V1.yaml /output:src/app/services/api-kpay-ccpayment.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromOperationId /ClassName:{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:KPAY_CCPAYMENT_API_URL && node ./api/api.js",
    "nswag-gen-kpay-fixedqr": "nswag openapi2tsclient /input:https://api.kpay.com.tr/fixedqr/KPay.API.FixedQR.V1.yaml /output:src/app/services/api-kpay-fixedqr.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromOperationId /ClassName:{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:KPAY_FIXEDQR_API_URL && node ./api/api.js",
    "nswag-gen-kpay-fixedqrpayment": "nswag openapi2tsclient /input:https://api.kpay.com.tr/fixedqrpayment/KPay.API.FixedQRPayment.V1.yaml /output:src/app/services/api-kpay-fixedqrpayment.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromOperationId /ClassName:{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:KPAY_FIXEDQRPAYMENT_API_URL && node ./api/api.js",
    "nswag-gen-wishalink": "nswag openapi2tsclient /input:https://panel.wishalink.com/apidoc/v1/api.json?key=wishalink /output:src/app/services/api-wishalink.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromPathSegments /ClassName:{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:WISH_API_URL && node ./api/api.js",
    "nswag-gen-wishalink-local": "nswag openapi2tsclient /input:https://localhost:44301/apidoc/v1/api.json?key=wishalink /output:src/app/services/api-wishalink.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromPathSegments /ClassName:{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:WISH_API_URL && node ./api/api.js"
  },
  "dependencies": {
    "@angular/cdk": "^12.2.12",
    "@angular/common": "~12.1.1",
    "@angular/core": "~12.1.1",
    "@angular/forms": "~12.1.1",
    "@angular/platform-browser": "~12.1.1",
    "@angular/platform-browser-dynamic": "~12.1.1",
    "@angular/router": "~12.1.1",
    "@capacitor-community/contacts": "^1.1.3",
    "@capacitor/android": "^3.2.5",
    "@capacitor/app": "1.0.3",
    "@capacitor/browser": "^1.0.5",
    "@capacitor/camera": "^1.1.2",
    "@capacitor/clipboard": "^1.0.5",
    "@capacitor/core": "3.2.4",
    "@capacitor/device": "^1.0.5",
    "@capacitor/filesystem": "^1.0.5",
    "@capacitor/geolocation": "^1.1.2",
    "@capacitor/haptics": "^1.1.0",
    "@capacitor/ios": "3.2.5",
    "@capacitor/keyboard": "^1.1.0",
    "@capacitor/local-notifications": "^1.0.8",
    "@capacitor/network": "^1.0.5",
    "@capacitor/push-notifications": "^1.0.7",
    "@capacitor/share": "^1.0.6",
    "@capacitor/splash-screen": "^1.1.5",
    "@capacitor/status-bar": "^1.0.6",
    "@capacitor/storage": "^1.2.2",
    "@capacitor/toast": "^1.0.5",
    "@ionic/angular": "^5.5.2",
    "angular-code-input": "^1.5.0",
    "cordova-plugin-screen-orientation": "^3.0.2",
    "es6-promise-plugin": "^4.2.2",
    "flag-icon-css": "^4.1.4",
    "ion-bottom-sheet": "^2.0.6",
    "ionic-selectable": "^4.9.0",
    "ionic2-calendar": "^0.6.9",
    "jwt-decode": "^3.1.2",
    "moment": "^2.29.1",
    "ngx-mask": "^12.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.2.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~12.1.1",
    "@angular-eslint/builder": "~12.0.0",
    "@angular-eslint/eslint-plugin": "~12.0.0",
    "@angular-eslint/eslint-plugin-template": "~12.0.0",
    "@angular-eslint/template-parser": "~12.0.0",
    "@angular/cli": "12.1.4",
    "@angular/compiler": "~12.1.1",
    "@angular/compiler-cli": "~12.1.1",
    "@angular/language-service": "~12.0.1",
    "@capacitor/cli": "3.2.4",
    "@ionic/angular-toolkit": "^4.0.0",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "4.16.1",
    "@typescript-eslint/parser": "4.16.1",
    "eslint": "^7.6.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.6",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "jasmine-core": "~3.8.0",
    "jasmine-spec-reporter": "~5.0.0",
    "jetifier": "^2.0.0",
    "karma": "~6.3.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.0.3",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "nswag": "^13.13.2",
    "protractor": "~7.0.0",
    "ts-node": "~8.3.0",
    "typescript": "~4.2.4"
  }
}
