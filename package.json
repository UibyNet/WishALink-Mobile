{
  "name": "wish-a-link",
  "version": "0.0.1",
  "author": "Wish a Link",
  "homepage": "https://wishalink.com/",
  "description": "Wish a Link - Mobile",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ionic build --prod",
    "sync": "ionic capacitor sync && node --max-old-space-size=4096 ./api/version.js",
    "resources": "cordova-res ios && cordova-res android && node --max-old-space-size=4096 ./api/resources.js",
    "ios-run-l": "ionic capacitor run ios -l --external",
    "android-run-l": "ionic capacitor run android -l --external",
    "android-open": "ionic capacitor update android && npx cap open android",
    "ios-open": "ionic capacitor update ios && npx cap open ios",
    "jetify": "npx jetify && npx cap sync android",
    "nswag-gen-kpay-backend": "nswag openapi2tsclient /input:https://api.kpay.com.tr/appbackend/KPay.API.AppBackend.V1.yaml /output:src/app/services/api-kpay-backend.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromFirstTagAndOperationId /ClassName:KpayBackend{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:KPAY_BACKEND_API_URL && node --max-old-space-size=4096 ./api/api.js",
    "nswag-gen-kpay-ccpayment": "nswag openapi2tsclient /input:https://api.kpay.com.tr/ccpayment/KPay.API.CCPayment.V1.yaml /output:src/app/services/api-kpay-ccpayment.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromFirstTagAndOperationId /ClassName:KpayCcpayment{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:KPAY_CCPAYMENT_API_URL && node --max-old-space-size=4096 ./api/api.js",
    "nswag-gen-kpay-fixedqr": "nswag openapi2tsclient /input:https://api.kpay.com.tr/fixedqr/KPay.API.FixedQR.V1.yaml /output:src/app/services/api-kpay-fixedqr.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromFirstTagAndOperationId /ClassName:KpayFixedqr{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:KPAY_FIXEDQR_API_URL && node --max-old-space-size=4096 ./api/api.js",
    "nswag-gen-kpay-fixedqrpayment": "nswag openapi2tsclient /input:https://api.kpay.com.tr/fixedqrpayment/KPay.API.FixedQRPayment.V1.yaml /output:src/app/services/api-kpay-fixedqrpayment.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromFirstTagAndOperationId /ClassName:KpayFixedqrPayment{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:KPAY_FIXEDQRPAYMENT_API_URL && node --max-old-space-size=4096 ./api/api.js",
    "nswag-gen-kpay": "npm run nswag-gen-kpay-backend && npm run nswag-gen-kpay-ccpayment && npm run nswag-gen-kpay-fixedqr && npm run nswag-gen-kpay-fixedqrpayment",
    "nswag-gen-wishalink": "nswag openapi2tsclient /input:https://panel.wishalink.com/apidoc/v1/api.json?key=wishalink /output:src/app/services/api-wishalink.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromPathSegments /ClassName:{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:WISH_API_URL && node --max-old-space-size=4096 ./api/api.js",
    "nswag-gen-wishalink-local": "nswag openapi2tsclient /input:https://localhost:44301/apidoc/v1/api.json?key=wishalink /output:src/app/services/api-wishalink.service.ts /Template:Angular /InjectionTokenType:InjectionToken /OperationGenerationMode:MultipleClientsFromPathSegments /ClassName:{controller}ApiService /DateTimeType:MomentJS /BaseUrlTokenName:WISH_API_URL && node --max-old-space-size=4096 ./api/api.js"
  },
  "dependencies": {
    "@angular/cdk": "^13.0.0",
    "@angular/common": "~13.0.0",
    "@angular/core": "~13.0.0",
    "@angular/forms": "~13.0.0",
    "@angular/platform-browser": "~13.0.0",
    "@angular/platform-browser-dynamic": "~13.0.0",
    "@angular/router": "~13.0.0",
    "@capacitor-community/contacts": "^1.1.3",
    "@capacitor/android": "^3.4.3",
    "@capacitor/app": "1.1.1",
    "@capacitor/browser": "^1.0.7",
    "@capacitor/camera": "^1.3.1",
    "@capacitor/clipboard": "^1.0.8",
    "@capacitor/core": "3.4.3",
    "@capacitor/device": "^1.1.2",
    "@capacitor/filesystem": "^1.1.0",
    "@capacitor/geolocation": "^1.3.1",
    "@capacitor/haptics": "1.1.4",
    "@capacitor/ios": "^3.4.3",
    "@capacitor/keyboard": "1.2.2",
    "@capacitor/local-notifications": "^1.1.0",
    "@capacitor/network": "^1.0.7",
